[sub_resource type="GDScript" id="GDScript_basket"]
script/source = "extends Sprite2D

var original_rotation: float = 0.0

func _ready():
	original_rotation = rotation

# √á√∂p sallama animasyonu
func shake():
	var tween = create_tween()
	tween.set_trans(Tween.TRANS_QUAD)
	tween.set_ease(Tween.EASE_IN_OUT)
	# Hafif sallan
	tween.tween_property(self, \"rotation\", original_rotation + deg_to_rad(8), 0.08)
	tween.tween_property(self, \"rotation\", original_rotation - deg_to_rad(8), 0.08)
	tween.tween_property(self, \"rotation\", original_rotation, 0.08)

# Drop kontrol√º - malzeme bƒ±rakƒ±ldƒ±ƒüƒ±nda
func check_drop(mouse_pos: Vector2) -> bool:
	var basket_rect = Rect2(global_position - texture.get_size() * scale / 2, texture.get_size() * scale)
	if basket_rect.has_point(mouse_pos):
		print(\"√á√∂pe atƒ±ldƒ±!\")
		shake()  # √á√∂p sallan
		return true
	return false
"

[sub_resource type="GDScript" id="GDScript_PotClick"]
script/source = "extends Sprite2D

func _input(event: InputEvent):
	if event is InputEventMouseButton and event.pressed and event.button_index == MOUSE_BUTTON_LEFT:
		var mouse_pos = get_global_mouse_position()
		var pot_rect = Rect2(global_position - texture.get_size() * scale / 2, texture.get_size() * scale)
		
		if pot_rect.has_point(mouse_pos):
			var cooking_manager = get_node('../CookingManager')
			if cooking_manager:
				cooking_manager.start_cooking()
"

